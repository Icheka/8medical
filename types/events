import { TTransaction } from "./transaction";
import { EUserTypes } from "./user";

export enum EEventTypes {
    transaction = "transaction",
    signup = "signup",
    login = "login",
    roundStatusChanged = "round_status_changed",
}

export interface TEvent {
    _id: string;
    userFocused: boolean;
    userId?: string;
    userType?: EUserTypes;
    eventType: EEventTypes;
    transactionId?: string;
    roundId?: string;
    transaction?: TTransaction;
    createdAt: Date;
    updatedAt: Date;
    meta?: any;
}
