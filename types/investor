import { EAccountActivityStatus, IUser } from "./common";

export interface TInvestor extends IUser {
    firstName: string;
    lastName: string;
    middleName?: string;
    emailVerified: boolean;
    activityStatus: EAccountActivityStatus;
    password: string;
    phone: string;
    expoPushTokens: string[];
    cashBalance: number;
}

export type TInvestorSignupPayload = {
    firstName: string;
    lastName: string;
    middleName?: string;
    email: string;
    password: string;
    expoPushTokens?: string[];
};
