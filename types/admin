import { EAccountActivityStatus, IUser } from "./common";

export interface TVestAdmin extends IUser {
    firstName: string;
    lastName: string;
    middleName?: string;
    emailVerified: boolean;
    activityStatus: EAccountActivityStatus;
}

export type TVestAdminSignupPayload = {
    firstName: string;
    lastName: string;
    middleName?: string;
    email: string;
    password: string;
};
